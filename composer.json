{
    "name": "mrshanebarron/otp-input",
    "description": "OTP/2FA code input component for Laravel",
    "keywords": ["laravel", "otp", "2fa", "code", "livewire", "vue"],
    "license": "MIT",
    "authors": [{"name": "Shane Barron", "email": "mrshanebarron@gmail.com"}],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.0"
    },
    "autoload": {
        "psr-4": {"LaravelDesign\\otp-input\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"LaravelDesign\\otp-input\\Tests\\": "tests/"}
    },
    "extra": {
        "laravel": {
            "providers": ["LaravelDesign\\otp-input\\otp-inputServiceProvider"]
        }
    },
    "config": {"sort-packages": true},
    "minimum-stability": "stable",
    "prefer-stable": true
}
